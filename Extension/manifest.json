{
	"manifest_version": 2,
	"name": "Mini Project",
	"description": "Project having 1.0 Credit",
	"version": "0.01",
	"author": "Umang Gupta",
	"background": {
		"scripts": ["background.js"],
		"persistent": false
	},
	"browser_action": {
		"default_popup": "index.html",
		"default_title": "Mini Project",
		"default_icon": {
			"16": "images/get_started16.png",
			"32": "images/get_started32.png",
			"48": "images/get_started48.png",
			"128": "images/get_started128.png"
		}
	},
	"content_scripts": [
		{
			"matches": ["https://twitter.com/*","http://twitter.com/*","https://www.twitter.com/*","http://www.twitter.com/*"],
			"js": ["jquery-3.4.1.min.js","content.js"]
		}
	],
	"permissions": ["storage","activeTab","declarativeContent","contextMenus"],
	"icons": {
		"16": "images/get_started16.png",
		"32": "images/get_started32.png",
		"48": "images/get_started48.png",
		"128": "images/get_started128.png"
	}
}